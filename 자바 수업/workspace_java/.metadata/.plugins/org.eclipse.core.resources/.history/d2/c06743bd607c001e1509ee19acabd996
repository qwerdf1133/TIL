
public class Practice06GuGuDan {
	
	public static void main(String[] args) {
		
		for(int i = 9; i > 1; i--) {
			System.out.print(9+" * "+i+" = "+ (9*i)+"\t");
		}
		System.out.println();
		for(int i = 9; i > 1; i--) {
			System.out.print(8+" * "+i+" = "+ (8*i)+"\t");
		}
		System.out.println();
		System.out.println("====================================================================");
		
		for(int j = 9; j >= 2; j--) {
			for(int i = 9; i > 1; i--) {
				System.out.print(j+" * "+i+" = "+ (j*i)+"\t");
			}
			System.out.println("===================================================================");
			for(int i = 2; i < 10; i++) {
				for(int j = 2; j <= 9; j++) {
					
				}
			}
				
		}
		
	}

}
