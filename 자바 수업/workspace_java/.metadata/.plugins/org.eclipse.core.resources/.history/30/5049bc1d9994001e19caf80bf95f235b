package ex01_nested_class;

// 실행 클래스
public class Main {

	public static void main(String[] args) {
		A a = new A();
		a.methodA();
		
		System.out.println("----------------------------");
		
		A.B b = a.new B();
		b.methodB();
		
		System.out.println("----------------------------");
		
		a.C c = new A.c();
	}

}
