package ex03_lambda_expression.param;

public class LambdaExample {

	public static void main(String[] args) {
		MyInterface i = new MyInterface() {

			@Override
			public void methodA(int x, int y) {
				int sum = x + y;
				System.out.println("sum : " + sum);
			}
			
		};
		
		i.methodA(1, 10);
		
		i = (int a, int b)->{
			
		};
		
		i.methodA(2, 3);

	}

}
